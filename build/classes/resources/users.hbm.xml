<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

	<class name="beans.Users" table="Users" schema="system">

		<id name="email" column="email"></id>
		<property name="fname" column="first_name"></property>
		<property name="lname" column="last_name"></property>
		<property name="mobile" column="mobile_no"></property>
		<property name="gender" column="gender"></property>
		<property name="password" column="password"></property>
	</class>

</hibernate-mapping>